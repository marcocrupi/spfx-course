{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfx-course-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxCourse/SpfxCourseWebPart.js",
          "manifest": "./src/webparts/spfxCourse/SpfxCourseWebPart.manifest.json"
        }
      ]
    },
    "render-all-pages-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/renderAllPages/RenderAllPagesWebPart.js",
          "manifest": "./src/webparts/renderAllPages/RenderAllPagesWebPart.manifest.json"
        }
      ]
    },
    "hello-world-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/helloWorld/HelloWorldApplicationCustomizer.js",
          "manifest": "./src/extensions/helloWorld/HelloWorldApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SpfxCourseWebPartStrings": "lib/webparts/spfxCourse/loc/{locale}.js",
    "RenderAllPagesWebPartStrings": "lib/webparts/renderAllPages/loc/{locale}.js",
    "HelloWorldApplicationCustomizerStrings": "lib/extensions/helloWorld/loc/{locale}.js"
  }
}
